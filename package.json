{
  "name": "novo-player",
  "version": "0.7.0",
  "main": "electron/dist/electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && tsc --p electron && electron .",
    "package-win": "electron-packager . --overwrite --no-tmpdir --platform=win32 --arch=x64  --prune=true --out=../release-builds --icon=./src/assets/icon.ico",
